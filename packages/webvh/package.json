{
  "name": "@credo-ts/webvh",
  "version": "0.5.13",
  "description": "Credo WebVH DID method implementation",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": ["build"],
  "publishConfig": {
    "access": "public",
    "main": "./build/index.js",
    "types": "./build/index.d.ts",
    "module": "./build/index.mjs",
    "exports": {
      ".": {
        "import": "./build/index.mjs",
        "require": "./build/index.js",
        "types": "./build/index.d.ts"
      },
      "./package.json": "./package.json"
    }
  },
  "license": "Apache-2.0",
  "scripts": {
